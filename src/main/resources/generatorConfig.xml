<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <classPathEntry location="C:\Users\lxl\.m2\repository\mysql\mysql-connector-java\5.1.47\mysql-connector-java-5.1.47.jar"/>
    <context id="testTable" targetRuntime="MyBatis3">
        <commentGenerator>
            <!--是否去除自动生成注释-->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库连接信息-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/xiaoyuanpe"
            userId="root" password="waylxl">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!--生成po类的位置-->
        <javaModelGenerator targetPackage="com.xiaoyuanpe.pojo" targetProject=".\src\main\java">
            <!--是否让shame作为包的后缀-->
            <property name="enableSubPackages" value="false"/>
            <!--从数据库返回的值清理前后的空格-->
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!--mapper映射文件生成位置-->
        <sqlMapGenerator targetPackage="com.xiaoyuanpe.mapper" targetProject=".\src\main\java">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>
        <!--接口生成的位置-->
        <javaClientGenerator targetPackage="com.xiaoyuanpe.mapper" type="XMLMAPPER" targetProject=".\src\main\java">
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>
